{
  "name": "sportshub-cli",
  "version": "1.0.0",
  "description": "CLI tool for Sportshub Leads to create Jira tickets",
  "main": "dist/index.js",
  "bin": {
    "sportshub": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "install-local": "npm run build && npm link"
  },
  "keywords": [
    "cli",
    "jira",
    "sportshub",
    "leads"
  ],
  "author": "Sportshub",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "cheerio": "^1.1.2",
    "commander": "^11.1.0",
    "inquirer": "^8.2.6"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.8.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
